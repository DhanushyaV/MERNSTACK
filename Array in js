//Array in js

let arrayy=[1,"aaa",1.4];
console.log(arrayy)

//Methods in js
//1)push-> add elements at the end

arrayy.push(100);
console.log(arrayy);
//2)pop-> remove elemnet at te end

arrayy.pop();
console.log(arrayy);

//3)shift->remove first element 
arrayt.shift();

//4)unshift()-> add element  at front
arrayy.unshift("abc");

//5)length-> return the length of an array

console.log(arrayy.length);

//6)->splice(index,delete count)

array.splice(2,1,101,102)
console.log(arrayy);
